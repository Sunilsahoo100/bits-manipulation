//https://practice.geeksforgeeks.org/problems/find-position-of-set-bit3706/1#

class Solution {
  public:
    int findPosition(int N) {
        // code here
        if(N==0)
        return -1;
        else if(((N-1) & N) == 0)
        { 
            return log2(N)+1;
        }
        else return -1;
        
    }
};
