//https://practice.geeksforgeeks.org/problems/bit-difference-1587115620/1

class Solution{
    public:
    // Function to find number of bits needed to be flipped to convert A to B
    int countBitsFlip(int a, int b){
        
        // Your logic here
        int ct=0;
        for(int i=0;i<32;i++)
        {
            if(((a^b) & (1<<i)) != 0)
            ct++;
        }
        return ct;
    }
};
