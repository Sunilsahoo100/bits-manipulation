https://practice.geeksforgeeks.org/problems/set-bits0143/1

class Solution {
  public:
    int setBits(int N) {
        int ct=0;
        for(int i=0;i<sizeof(int)*8;i++)
        {
            if((N & (1<<i)) != 0)
            ct++;
        }
        return ct;
        
        
        // Write Your Code here
    }
};
